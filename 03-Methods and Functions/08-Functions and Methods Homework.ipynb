    "import string\n",
    "\n",
    "def ispangram(str1, alphabet=string.ascii_lowercase):\n",
    "    print(f\"alphabet: {alphabet}\")\n",
    "    str1 = str1.lower()\n",
    "    for letter in str1:\n",
    "        if letter in alphabet:\n",
    "            alphabet = alphabet.replace(letter, '')\n",
    "    if len(alphabet) == 0:\n",
    "        print(\"Is a pangram\")\n",
    "    else:\n",
    "        print(\"Not a pangram\")\n",
    "    print(f\"alphabet: {alphabet} \")"
   ]
